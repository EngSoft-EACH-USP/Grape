<%= form_with scope: :user, url: users_path, local: true  do |form| %>
  <p>
    <%= form.label :Nome %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label "Email" %><br>
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label "Senha" %><br>
    <%= form.text_field :password %>
  </p>
  <p>
    <%= form.label "Confirmar senha" %><br>
    <%= form.text_field :password_conf %>
  </p>
  <p>
    <%= form.submit 'Registrar' %>
  </p>
<% end %>